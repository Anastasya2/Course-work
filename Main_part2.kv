<StackButton@MDFloatingActionButton>
    pos: self.parent.center_x - self.width / 2, "56dp"



<SevenWindow>:
    name: 'seven'
    md_bg_color: app.theme_cls.bg_light

    MDBoxLayout:
        orientation: 'vertical'
        padding: '12dp','15dp','12dp','12dp'
        spacing: "24dp"

        MDLabel:
            text: 'Контролируй свой диабет!'
            bold: True
            font_style: 'H5'
            adaptive_height: True
            padding_x: '24dp'

        ScrollView:
            MDList:
                id:hero_list
                padding: '24dp'
                spacing: '24dp'

<EightWindow>:
    name: "Piechart"
    FitImage:
        source: "bg_pie.png"

    MDBoxLayout:
        orientation: "vertical"

        MDTopAppBar:
            title: 'График питания     '
            md_bg_color: (252/255, 116/255, 168/255, 1)
            elevation: 4
            left_action_items: [["arrow-left", lambda x:root.set_screen()]]
            right_action_items: [["refresh", lambda x: root.update_chart()],["home", lambda x: app.back_to_window_seven(x)]]

        ScrollView:

            MDBoxLayout:
                orientation: "vertical"
                MDBoxLayout:
                    padding: "12dp"
                    orientation: "vertical"

                    MDLabel:
                        id: wel
                        font_size: 23
                        size_hint: 1,.3
                        padding_y: 240

                    MDLabel:
                        id: wel1
                        font_size: 23
                        size_hint: 1,.3
                        padding_y: 245

                MDBoxLayout:
                    id: chart_box
                    adaptive_height: True
                    padding: "24dp"
                    orientation: "vertical"


<NineWindow>:
    name: 'nine'
    hero_to:hero_to
    md_bg_color: app.theme_cls.bg_light

    MDBoxLayout:
        orientation:'vertical'
        spacing: "1dp"
        padding: "1dp"

        MDTopAppBar:
            id: toolbar
            size_hint_y: None
            height: "7dp"
            md_bg_color: 'white'
            title: 'Физическая активность'
            font_size: 10
            left_action_items: [["arrow-left", lambda x:app.back_to_window_seven()]]
            specific_text_color: 0,0,0,1

        MDHeroTo:
            id: hero_to
            size_hint_y: None
            height: "80dp"
            md_bg_color: 'gray'
            FitImage:
                source: "physics_in.png"

        GridLayout:
            cols:2
            padding:'30dp','1dp','30dp','1dp'
            size_hint: 1,.99

            MDTextField:
                id: pa
                helper_text: "Время физической активности"
                helper_text_mode: 'on_focus'
                text_color_focus: (62/255, 150/255,18/255,1)
                helper_text_color: "black"
                line_color_normal: "black"
                line_color_focus: (62/255, 150/255,18/255,1)

            MDLabel:
                text: 'мин.'
                font_size: 20
                halign: 'center'
                padding_y: 19

            MDTextField:
                id: pa2
                helper_text: "Расход калорий"
                helper_text_mode: 'on_focus'
                text_color_focus: (62/255, 150/255,18/255,1)
                helper_text_color: "black"
                line_color_normal: "black"
                line_color_focus: (62/255, 150/255,18/255,1)

            MDLabel:
                text: 'ккал'
                font_size: 20
                halign: 'center'
                padding_y: 19

        Widget:
            size_hint: 1,.5

        GridLayout:
            cols:2
            padding:'10dp','1dp','10dp','1dp'
            spacing:'3dp'
            size_hint: 1,.3

            MDRectangleFlatButton:
                spacing: 12
                font_size: 16
                text: "Ввести \nминуты \nфизической \nактивности"
                size_hint:(.2,.1)
                #md_bg_color: (255/255, 79/255, 79/255, 1)
                pos_hint: {"center_x": .5, "center_y": .5}
                theme_text_color: "Custom"
                text_color: 'red'
                line_color: 'red'
                on_press: app.physlog()

            MDRectangleFlatButton:
                spacing: 12
                font_size: 16
                text: "Ввести \nзатраченные \nкалории"
                size_hint:(.2,.1)
                #md_bg_color: (255/255, 79/255, 79/255, 1)
                pos_hint: {"center_x": .5, "center_y": .5}
                theme_text_color: "Custom"
                text_color: 'red'
                line_color: 'red'
                on_press: app.physlog2()

        Widget:
            size_hint: 1,.3

        AnchorLayout:
            anchor_x:'right'
            anchor_y:'bottom'
            padding:'10dp','10dp','10dp','10dp'

            MDFloatingActionButtonSpeedDial:
                data: app.physics_data
                root_button_anim: True
                md_bg_color: (62/255, 150/255,18/255,1)
                callback: app.physical_activity_buttons


